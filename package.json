{
  "name": "e-source-code-react-js-profile-app",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@8.15.9+sha512.499434c9d8fdd1a2794ebf4552b3b25c0a633abcee5bb15e7b5de90f32f47b513aca98cd5cfd001c31f0db454bc3804edccd578501e4ca293a6816166bbd9f81",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "start": "npm run build && vite preview",
    "format": "prettier --write \"./src/**/*.{ts,tsx,json}\"",
    "analyze": "npm run lint && tsc && vite build --mode=analyze && source-map-explorer 'dist/assets/*.js'",
    "verify-commit": "verify-commit-msg",
    "postinstall": "git-scm-hooks",
    "release": "bumpp -r",
    "lint": "eslint src",
    "check": "prettier --check src",
    "gen-icon": "csvgtocss -c icon",
    "fix": "eslint --fix src && prettier --write src",
    "gen-color": "esno ./scripts/genColorCss.ts"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@hunghg255/eslint-config": "^0.0.1",
    "@splidejs/react-splide": "^0.7.12",
    "@splidejs/splide-extension-auto-scroll": "^0.5.3",
    "@total-typescript/ts-reset": "^0.4.2",
    "@types/node": "^20.17.1",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@vitejs/plugin-react": "^4.0.4",
    "antd": "^5.21.5",
    "autoprefixer": "^10.4.20",
    "axios": "^1.7.7",
    "bumpp": "^9.2.0",
    "classnames": "^2.5.1",
    "csvgtocss": "^0.1.1",
    "eslint": "^9.13.0",
    "esno": "^4.8.0",
    "git-scm-hooks": "^0.0.6",
    "i18next": "^23.16.4",
    "i18next-browser-languagedetector": "^8.0.0",
    "i18next-http-backend": "^2.6.2",
    "jotai": "^2.10.1",
    "postcss": "^8.4.47",
    "prettier": "^2.8.8",
    "react-i18next": "^15.1.0",
    "react-router-dom": "^6.27.0",
    "react-simple-typewriter": "^5.0.1",
    "sass": "^1.80.3",
    "source-map-explorer": "^2.5.3",
    "tailwindcss": "^3.4.14",
    "typescript": "^5.6.3",
    "verify-commit-msg": "^0.0.11",
    "vite": "^4.4.9",
    "vite-plugin-checker": "^0.6.2",
    "vite-plugin-environment": "^1.1.3",
    "vite-plugin-lightningcss": "^0.0.5"
  },
  "browserslist": [
    ">0.3%",
    "not ie 11",
    "not dead",
    "not op_mini all"
  ],
  "git-hooks": {
    "pre-commit": "npm run lint",
    "commit-msg": "npm run verify-commit"
  }
}
